(module jazz.validation.validate.Validation-Manager jazz
  (import (jazz.log))
  (import (jazz.markup))
  (import (jazz.validation))
  (class Validation-Manager extends Object
    (slot feedback)
    (slot feedback-port)
    (slot report-success?)
    (slot report-results?)
    (slot log getter generate)
    (method override (initialize self (feedback: feedback (unspecified)) (feedback-port: feedback-port (unspecified)) (report-success?: report-success? (unspecified)) (report-results?: report-results? (unspecified)) (log: log (unspecified))))
    (method (default-feedback self format-string . rest))
    (method public (validate self suite))
    (method public (log-failure self suite tester section message class location properties))
    (method public (get-failures self))
    (method public (report-results self)))
  (hub get-log)
  (hub validate)
  (hub log-failure)
  (hub get-failures)
  (hub report-results))
