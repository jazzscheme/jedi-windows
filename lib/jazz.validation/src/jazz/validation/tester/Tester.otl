(module jazz.validation.tester.Tester jazz
  (import (jazz.component))
  (import (jazz.debuggee.view))
  (import (jazz.io))
  (import (jazz.snapshot))
  (import (jazz.validation))
  (class Tester extends Component
    (property manager accessors generate)
    (property suite accessors generate)
    (property title accessors generate)
    (method public virtual (test self))
    (method package (invoke self section proc))
    (method (validation-failed-with-snapshot self exception (message (unspecified))))
    (method (validate-failed self message location))
    (method (validation-failed self message (class: class (unspecified)) (location: location (unspecified)) (properties: properties (unspecified)))))
  (hub get-manager)
  (hub set-manager)
  (hub get-suite)
  (hub set-suite)
  (hub get-title)
  (hub set-title)
  (hub test)
  (hub invoke))
