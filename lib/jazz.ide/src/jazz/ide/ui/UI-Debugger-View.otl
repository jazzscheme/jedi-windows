  (hub get-events)
  (hub get-selection)
  (hub get-layers)
  (hub get-layer-regions)
  (hub get-layer-order)
  (hub get-tree)
  (hub on-refresh)
  (hub on-clear)
  (hub spy-hook)
  (hub play-events)
  (hub play-move)
  (class UI-Debugger-Render extends View
    (method override (draw self surface context)))
  (class Spy-Event extends Object
    (method override (initialize self layer action arguments))
    (slot layer getter generate)
    (slot action getter generate)
    (slot arguments getter generate))
  (hub get-layer)
  (hub get-action)
  (hub get-arguments))
