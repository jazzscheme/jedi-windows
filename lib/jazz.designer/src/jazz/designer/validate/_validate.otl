(module jazz.designer.validate jazz
  (import (jazz.component))
  (import (jazz.markup))
  (import (jazz.runtime))
  (import (jazz.validation))
  (class Designer-Descriptors-Tester extends Tester
    (definition class-form)
    (method meta override (set-class-form self form))
    (method meta override (get-class-form self))
